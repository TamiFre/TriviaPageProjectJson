<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="LoginPage.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:LoginPage.Views"
    Shell.FlyoutBehavior="Flyout">

    <ShellContent
        Title="Login" 
        ContentTemplate="{DataTemplate local:LoginPageView}"
        Route="LoginPage"
         />
    
  
    <ShellContent
        Title="Approve Questions"
        ContentTemplate="{DataTemplate local:ApproveQuestionsPageView}"
        Route="ApproveQuestionsPage"
         />
    
    
    <ShellContent
        Title="User question page"
        ContentTemplate="{DataTemplate local:UserQuestionsPageView}"
        Route="UserQuestionPage" 
         />

    <ShellContent
        Title="Admin Page"
        ContentTemplate="{DataTemplate local:UserAdminPageView}"
        Route="UserAdminPage" />




</Shell>
